# Cloudinary Configuration
# Copy this file to config.env and fill in your values

# Your Cloudinary Cloud Name (found in dashboard)
CLOUDINARY_CLOUD_NAME=your_cloud_name

# API Key (found in dashboard -> Settings -> Access Keys)
CLOUDINARY_API_KEY=your_api_key

# API Secret (found in dashboard -> Settings -> Access Keys)
CLOUDINARY_API_SECRET=your_api_secret

# Optional: Folder to organize uploaded images
CLOUDINARY_FOLDER=product-images
